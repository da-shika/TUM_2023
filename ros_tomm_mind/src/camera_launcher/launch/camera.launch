<?xml version="1.0"?>

<launch>

  <!-- static transformation of real robot to robot model --> 
  <node pkg="tf" type="static_transform_publisher" name="camera_static_transform" args="0 0 0 0 0 0 tomm/tomm/asus_link camera_link 10" />

  <include file="$(find openni2_launch)/launch/openni2.launch">
    <arg name="rgb_camera_info_url" value="file://$(find camera_launcher)/launch/config/rgb_PS1080_PrimeSense.yaml"/>
  </include>

</launch>
